# flaresolverr.env
TZ: Etc/UTC
LOG_LEVEL: info
# Maximum number of browser instances (default: 1)
MAX_INSTANCES: 2
# Maximum number of concurrent requests per instance (default: 1)
MAX_CONCURRENT_REQUESTS: 1
# Timeout for solving challenges (default: 60000ms)
REQUEST_TIMEOUT: 60000
# Enable debug mode (default: false)
DEBUG: false
